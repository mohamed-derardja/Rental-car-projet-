public class Car {
        private int id;
        private String model;
        private boolean available;
        private int price;

        public Car(int id, String model, boolean available , int price) {
            this.id = id;
            this.model = model;
            this.available = available;
            this.price = price;
        }

        public int getId() {
            return id;
        }
        public void setId(int id) {
            this.id = id;
        }
        public String getModel() {
            return model;
        }
        public void setModel(String model) {
            this.model = model;
        }
        public boolean isAvailable() {
            return available;
        }
        public void setAvailable(boolean available) {
            this.available = available;
        }
        public int getPrice(){
            return price;
        }
        public void setPrice(int price){
            this.price = price;
        }
    }
    

}
